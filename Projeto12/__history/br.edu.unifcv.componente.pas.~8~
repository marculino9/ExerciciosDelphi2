unit br.edu.unifcv.componente;

interface

uses
  System.classes;

procedure register;

type
  TEventos = class(TComponent)
  private
    FNome: String;

  published
    property Nome: String read FNome write Fnome;

  end;

implementation

procedure register;
begin
  RegisterComponents('Faculdade', [TEventos]);
end;

end.
